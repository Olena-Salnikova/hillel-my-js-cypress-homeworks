  },
  "homepage": "https://github.com/Olena-Salnikova/hillel-my-js-cypress-homeworks#readme",
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "cypress": "^15.6.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2"
  }
}
