{
  "name": "hillel-my-js-cypress-homeworks",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "open": "npx cypress open",
    "test": "npx cypress run",
    "test:cloud": "npx cypress run --record --key 6a8b8b2e-1f3f-4e2f-85e3-5f3d1f5e6c7d",
    "merge-reports": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
    "generate-report": "npx marge mochawesome.json",
    "report": "npm run merge-reports && npm run generate-report",
    "test-env-qauto": "npx cypress open  --config-file cypress.qauto.config.js",
    "test-env-qauto2": "npx cypress open  --config-file cypress.qauto2.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Olena-Salnikova/hillel-my-js-cypress-homeworks.git"
